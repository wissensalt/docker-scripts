{
  "name": "postgres-expired_date_details-source",
  "config": {
    "connector.class": "io.debezium.connector.postgresql.PostgresConnector",
    "tasks.max": "1",
    "plugin.name": "pgoutput",

    "database.hostname": "postgres",
    "database.port": "5432",
    "database.user": "postgres",
    "database.password": "pgadmin",
    "database.dbname": "kf_stock",
    "database.server.name": "postgre-kf_stock",
    "database.server.id": "5401",
    "database.history.kafka.bootstrap.servers": "kafka:9092",
    "database.history.kafka.topic": "public.history",

    "topic.prefix": "public",
    "table.include.list": "public.expired_date_details",
    "include.schema.changes": "true",    
    "time.precision.mode":"connect",

    "transforms":"Reroute",
    "transforms.Reroute.type": "io.debezium.transforms.ByLogicalTableRouter",
    "transforms.Reroute.topic.regex":"(.*)expired_date_details",
    "transforms.Reroute.topic.replacement": "public.expired_date_details",
    "transforms.Reroute.key.field.name": "id",
    "transforms.Reroute.key.enforce.uniqueness":"false"
  }
}